
<div class="mb-3">
<%= form.label :Bookstore, "Book store id" %>
<%= form.number_field :bookstore_id, class: "form-control", placeholder: " bookstore id" %>
</div>
<div class="mb-3">
<%= form.label :name, "Book Name" %>
<%= form.text_field :name, class: "form-control", placeholder: "Bookstore Name" %>
</div>
<div class="mb-3">
<%= form.label :author, "Author" %>
<%= form.text_field :author, class: "form-control", placeholder: "author" %>
</div>
<div class="mb-3">
<%= form.label :description, " description" %>
<%= form.text_field :description, class: "form-control", placeholder: "Description" %>
</div>
<div class="mb-3">
<%= form.label :Price, "Price" %>
<%= form.number_field :price, class: "form-control", placeholder: " price" %>
</div>
<div class="mb-3">
<%= form.label :quantity, "Quantity" %>
<%= form.number_field :quantity, class: "form-control", placeholder: " quantity" %>
</div>

<div class="mb-3">
<%= form.label :image, "image" %>
<%= form.file_field :image, class: "form-control", placeholder: " image" %>
</div>

